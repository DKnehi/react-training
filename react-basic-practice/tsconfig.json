{
  "compilerOptions": {
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es5",
    "module": "es6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@chakra-ui/react": ["node_modules/@chakra-ui/react"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
